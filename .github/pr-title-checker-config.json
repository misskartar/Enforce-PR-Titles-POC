{
  "LABEL": {
    "name": "PR Title Needs Formatting",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "regexp": "(build|chore|ci|docs|feat|fix|perf|refactor|revert|style|test)(\\\\([a-z0-9\\\\s]+\\\\))?(:\\\\s)([a-z0-9\\\\s]+)",
    "regexpFlags": "i",
    "prefixes": [
      "bugfix: ",
      "feature: "
    ],
    "ignoreLabels": [
      "skip-pr-title-check"
    ]
  },
  "MESSAGES": {
    "success": "PR Title OK",
    "failure": "Failing PR Title Check",
    "notice": ""
  }
}